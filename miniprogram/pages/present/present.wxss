.present.p {}
    .p.gallery_box {width:100%;height:100vh;}
    .p.gallery {width:100%;height:100vh;background-color:#fff;}
    .p.bg_image{width:100%;height:100vh;}
    .p.logo{width:500rpx;height:140rpx;position:absolute;top:20%;left:50%;transform:translate(-50%,-50%);
        animation:fadeIn 4s;
    }
    .p.invite{width:100%;font-size:26rpx;text-align:center;color:#eee;position:absolute;top:30%;left:50%;transform:translate(-50%,-50%);
        animation:fadeIn2 4s;
    }
    .p.md_image{position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);z-index:10;}
    .p.top_image{position:absolute;}
    .p.p2{width:750rpx;height:761rpx;top:65%;transform:translate(0,-760rpx)}
    .p.p3{width:750rpx;height:623rpx;}
    .p.p4{width:750rpx;height:688rpx;opacity:0.7;}
    .p.p5{width:750rpx;height:576rpx;}
    .p.btn{width:240rpx;height:72rpx;border:1px solid #e9418e;color:#e9418e;border-radius:10rpx;font-size:30rpx;text-align:center;line-height:72rpx;margin:14rpx 20rpx;display:inline-block;box-sizing:border-box;}
    .p.btn_box{width:560rpx;position:absolute;left:50%;top:65%;transform:translate(-50%,20%)}
    .p.choose{font-size:24rpx;color:#999;position:absolute;left:50%;top:85%;transform:translate(-50%,20%)}
    .p.triangle{width:12rpx;border-bottom:12rpx solid #999;border-left:12rpx solid transparent;border-right:12rpx solid transparent;position:relative;top:-20rpx;margin-left:10rpx;}
    .p.title_box{width:100%;position:absolute;top:18%;left:50%;transform:translate(-50%,-50%)}
    .p.title{width:100%;font-size:52rpx;color:#e9418e;text-align:center;margin-bottom:32rpx;}
    .p.subtitle{width:100%;font-size:24rpx;color:#aaa;text-align:center;}
    .p.label_box{width:100%;text-align:center;position:absolute;top:82%;left:50%;transform:translate(-50%,-50%)}
    .p.label_line{}
    .p.label,.p.label2{display:inline-block;color:#999;padding:0 30rpx;margin-bottom:10rpx;border-right:1rpx solid #999;}
    .p.label{width:180rpx;padding:0;}
    .p.label:last-child,.p.label2:last-child{border:none;}
    .p.btn2{font-size:20rpx;padding:10rpx 30rpx;border:1rpx solid #999;border-radius:8rpx;color:#999;display:inline-block;margin-top:48rpx;}
    .p.bg_2{width:100%;height:50vh;position:absolute;bottom:0;left:0;background-color:#444444;z-index:0;}
    .p.reverse{color:#fff;border-color:#fff;}
    .p.smile{width:200rpx;height:200rpx;position:absolute;left:50%;top:20%;transform:translate(-50%,-50%)}
    .p.title_box2{width:100%;position:absolute;left:50%;top:36%;transform:translate(-50%,-50%)}
    .p.title_box3{width:100%;position:absolute;left:50%;top:50%;transform:translate(-50%,-50%)}
    .p.subtitle2{width:100%;font-size:30rpx;color:#888;text-align:center;}

    .p.tab_box{width:100%;position:absolute;top:56%;left:50%;transform:translate(-50%,0)}
    .p.tab_line{width:calc(100% - 200rpx);padding-right:50rpx;display:inline-block;vertical-align:top;}
    .p.tab_title{width:100rpx;font-size:30rpx;padding-left:50rpx;color:#999;display:inline-block;vertical-align:top;line-height:54rpx;}
    .p.tab{font-size:26rpx;padding:8rpx 24rpx;border:1rpx solid #999;border-radius:10rpx;color:#999;display:inline-block;margin-right:20rpx;margin-bottom:20rpx;}
    .p.tab_select{color:#e9418e;border-color:#e9418e;}
    .p.arrow{width:40rpx;height:40rpx;border-left:4rpx solid #ccc;border-bottom:4rpx solid #ccc;position:fixed;bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);
        animation:arrow 4s infinite linear;
    }
    .p.bottom{width:100%;height:100rpx;position:fixed;bottom:0;left:0;}
    .p.a{}

    .p.btn_box3{width:100%;position:absolute;bottom:120rpx;text-align:center;
        animation:fadeIn3 4s;}
    .p.btn3{font-size:28rpx;padding:10rpx 40rpx;border:1px solid #e9418e;color:#e9418e;border-radius:10rpx;display:inline-block;margin:0 20rpx;}
    .p.w{}

@keyframes fadeIn {
    0%   {opacity:0}
    45%  {opacity:1}
    100% {opacity:1}
}

@keyframes fadeIn2 {
    0%   {opacity:0}
    15%  {opacity:0}
    60%  {opacity:1}
    100% {opacity:1}
}

@keyframes fadeIn3 {
    0%   {opacity:1;left:50%;transform:translate(-50%,0);}
    25%   {opacity:1;left:50%;transform:translate(-50%,0);}
    70%   {opacity:1;left:50%;transform:translate(-50%,0);}
    100%   {opacity:1;left:50%;transform:translate(-50%,0);}
}

@keyframes arrow {
/*bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);*/
    0%   {bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);opacity:0;}
    25%   {bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);opacity:0.5;}
    70%   {bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);opacity:0.5;}
    100%   {bottom:50rpx;left:50%;transform:translate(-50%,-50%) rotate(-45deg);opacity:0;}
}

button {width:100rpx;background-color: #fff;font-weight:normal;height:68rpx;border:1px solid #e9418e;color:#e9418e;display:inline-block;}
button:not([size='mini']){width:240rpx;height:68rpx;min-height:68rpx;padding:0;line-height:68rpx;margin:0 20rpx;}
